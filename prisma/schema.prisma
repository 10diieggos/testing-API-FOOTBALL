generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model countries {
  id      Int     @default(autoincrement()) @id
  country String?
  code    String?
  flag    String?
}

model brasil_leagues {
  id      Int   @default(autoincrement()) @id
  leagues Json?
}

model teams_SerieA_2020 {
  id    Int   @default(autoincrement()) @id
  teams Json?
}
